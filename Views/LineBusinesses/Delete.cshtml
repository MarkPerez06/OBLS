@model OBLS.Models.LineBusiness

@{
    ViewBag.Title = "Line of Business";
    ViewBag.pTitle = "Line of Business";
    ViewBag.pageTitle = "Delete";
}


<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-body">
                <h5>Are you sure you want to delete this?</h5>
                <hr />
                <dl class="row">
                    <dt class="col-sm-3">
                        @Html.DisplayNameFor(model => model.Code)
                    </dt>
                    <dd class="col-sm-9">
                        @Html.DisplayFor(model => model.Code)
                    </dd>
                    <dt class="col-sm-3">
                        @Html.DisplayNameFor(model => model.Description)
                    </dt>
                    <dd class="col-sm-9">
                        @Html.DisplayFor(model => model.Description)
                    </dd>
                    <dt class="col-sm-3">
                        @Html.DisplayNameFor(model => model.SignageBillboard_Capacity)
                    </dt>
                    <dd class="col-sm-9">
                        @Html.DisplayFor(model => model.SignageBillboard_Capacity)
                    </dd>
                    <dt class="col-sm-3">
                        @Html.DisplayNameFor(model => model.SignageBillboard_NoOfUnits)
                    </dt>
                    <dd class="col-sm-9">
                        @Html.DisplayFor(model => model.SignageBillboard_NoOfUnits)
                    </dd>
                    <dt class="col-sm-3">
                        @Html.DisplayNameFor(model => model.WeightsAndMeasures_Capacity)
                    </dt>
                    <dd class="col-sm-9">
                        @Html.DisplayFor(model => model.WeightsAndMeasures_Capacity)
                    </dd>
                    <dt class="col-sm-3">
                        @Html.DisplayNameFor(model => model.WeightsAndMeasures_NoOfUnits)
                    </dt>
                    <dd class="col-sm-9">
                        @Html.DisplayFor(model => model.WeightsAndMeasures_NoOfUnits)
                    </dd>
                </dl>

                <form asp-action="Delete">
                    <input type="hidden" asp-for="Id" />
                    <a asp-action="Index" class="btn btn-secondary">Back</a>
                    <input type="submit" value="Delete" class="btn btn-danger" />
                </form>
            </div>
        </div>
    </div>
</div>
